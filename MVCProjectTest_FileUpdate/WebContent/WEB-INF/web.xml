<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>MVCProjectTest</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>member</servlet-name>
  	<servlet-class>
  		Controller.Member.MemberController
  	</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>member</servlet-name>
  	<url-pattern>*.nhn</url-pattern>	
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>answerboard</servlet-name>
  	<servlet-class>
  		Controller.Answer.AnswerController
  	</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>answerboard</servlet-name>
  	<url-pattern>*.ab</url-pattern>	
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>board</servlet-name>
  	<servlet-class>
  		Controller.Board.BoardController
  	</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>board</servlet-name>
  	<url-pattern>*.bo</url-pattern>	
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>goods</servlet-name>
  	<servlet-class>
  		Controller.Goods.GoodsController
  	</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>goods</servlet-name>
  	<url-pattern>*.gd</url-pattern>	
  </servlet-mapping>
</web-app>